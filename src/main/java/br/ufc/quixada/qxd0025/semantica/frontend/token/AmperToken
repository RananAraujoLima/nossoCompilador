package br.ufc.quixada.qxd0025.semantica.frontend.token;

public class AmperToken extends Token {


    protected AmperToken(long line, long start, String value) throws IllegalArgumentException {
        super(line, start, value);
    }

    @Override
    public void interpretAttributes() {
        stringValue = stringValue.substring(0);
    }

    public String getAmperSpecialsymbols(){
        interpretAttributes();

        return stringValue;


    }
    @Override
    public String getTokenIdentifier() {
        return "AMPER";
    }
}
